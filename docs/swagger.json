        "api.DeleteProblemQ": {
            "type": "object"
        },
        "api.GetCaptchaQ": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string"
                }
            }
        },
        "api.GetProblemListQ": {
            "type": "object"
        },
        "api.GetProblemQ": {
            "type": "object"
        },
        "api.GetProblemVersionQ": {
            "type": "object"
        },
        "api.GetProfileA": {
